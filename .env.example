# Environment Variables for FOGG Calendar Deployment Dashboard
# Copy this file to .env.local for development

# Development configuration
VITE_APP_TITLE=FOGG Calendar Update Center
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=development

# API Configuration
VITE_API_BASE_URL=http://localhost:8888/.netlify/functions

# Feature flags
VITE_ENABLE_CONFETTI=true
VITE_ENABLE_ANIMATIONS=true
VITE_POLLING_INTERVAL=2000

# Deployment settings
VITE_DEFAULT_USER=Leslie
VITE_DEFAULT_ENVIRONMENT=production
VITE_DEPLOYMENT_TIMEOUT=30000

# Analytics (optional)
# VITE_ANALYTICS_ID=your-analytics-id

# Debug settings
VITE_DEBUG_MODE=false
VITE_SHOW_CONSOLE_LOGS=false